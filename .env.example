# Edwards Project Operation Board - Environment Variables

# Database (PostgreSQL - Local Development)
DATABASE_URL=postgresql://postgres:password@localhost:5432/edwards
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=edwards

# Database Reset (set to true to reset DB on startup)
RESET_DB=false

# Backend
SECRET_KEY=edwards-secret-key-change-in-production-use-random-string
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
DEBUG=true

# Frontend
VITE_API_URL=http://localhost:8000/api
